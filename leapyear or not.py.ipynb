   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "enter year:2020\n",
      "2020 is leap year\n"
     ]
    }
   ],
   "source": [
    "from calendar import*\n",
    "y=int(input(\"enter year:\",))\n",
    "if (isleap(y)):\n",
    "    print(y,\"is leap year\")\n",
    "else:\n",
    "    print(y,\"is not leap\")"
   ]

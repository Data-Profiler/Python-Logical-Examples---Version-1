     "text": [
      "x=  10\n",
      "x=  9\n",
      "x=  8\n",
      "x=  7\n",
      "x=  6\n",
      "out of loop\n"
     ]
    }
   ],
   "source": [
    "x= 10\n",
    "while x>=1:\n",
    "    print('x= ',x)\n",
    "    x-=1\n",
    "    if x==5:\n",
    "        break\n",
    "print(\"out of loop\")    "

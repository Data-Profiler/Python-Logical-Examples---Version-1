     ]
    }
   ],
   "source": [
    "max = int(input(\"upto what number?: \"))\n",
    "\n",
    "for num in range(2,max+1):\n",
    "    for i in range(2,num):\n",
    "        if(num % i) == 0:\n",
    "            break\n",
    "        else:\n",
    "            print(num)\n",
    "        "
